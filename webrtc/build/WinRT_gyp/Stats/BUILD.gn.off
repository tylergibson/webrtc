import("../../../webrtc.gni")

if(is_winrt) {
  rtc_static_library("webrtc_stats_observer") {
    sources = [
      "webrtc_stats_observer.cpp",
      "webrtc_stats_observer.h",
      "webrtc_stats_network_sender.h",
      "webrtc_stats_network_sender.cpp",
      "etw_providers.man",
    ]

    deps = [
          "../../../../webrtc/api:libjingle_peerconnection",
    ]
    
    if(rtc_build_json) {
      deps += [ "//third_party/jsoncpp" ]
    }
  }
}